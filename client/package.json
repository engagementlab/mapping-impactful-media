{
  "name": "mapping-impactful-media",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:ssr": "run-p routify nollup",
    "dev:tailwind": "cross-env PURGE=false postcss public/tailwind.css -o public/index.css -w",
    "dev:api": "source ~/.nvm/nvm.sh; cd ../api; nvm use; yarn dev",
    "build": "run-s build:*",
    "build:app": "rollup -c",
    "build:tailwind": "cross-env NODE_ENV=production PURGE=true postcss public/tailwind.css -o dist/index.css",
    "build:cleanup": "mv dist/__app.html dist/index.html; cp public/routes.json dist/routes.json",
    "start": "spassr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@roxi/routify": "^2.12.4",
    "@tsconfig/svelte": "^1.0.10",
    "cross-env": "^7.0.2",
    "eslint-config-airbnb-base": "14.2.1",
    "fs-extra": "^9.0.1",
    "nollup": "^0.16.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^13.0.0",
    "rollup": "^2.33.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-svelte-hot": "^0.11.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "spank": "^1.5.1",
    "spassr": "^2.2.0",
    "svelte": "^3.29.4",
    "svelte-preprocess": "^4.5.2",
    "tailwindcss": "^2.0.4",
    "tossr": "^1.3.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@cloudinary/base": "^1.0.0-beta.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "node-sass": "^5.0.0",
    "sass": "^1.32.8",
    "svelte-markdown": "^0.1.5"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {},
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  }
}
